    "repository": "https://github.com/szteambot",
    "logo": "https://www.herokucdn.com/deploy/button.png",
    "keywords": [
        "Bot",
        "Telegram ",
        "Pyrogram "
    ],
    "env": {
        "API_ID": {
            "description": "Get from my.telegram.org/apps",
